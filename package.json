    "eslint-plugin-react-refresh": "0.4.14",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "cspell": "8.14.4",
    "@commitlint/cli": "19.6.0",
    "@commitlint/config-conventional": "19.6.0",
    "commitizen": "4.3.1",
    "cz-git": "1.10.1",
